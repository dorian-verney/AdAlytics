---
import Layout from '../layouts/Layout.astro';
import MainPage from '../components/main_page/MainPage.svelte';

---

<Layout>
	<script is:inline>
		if (typeof localStorage === "undefined" || !localStorage.getItem("loggedIn")) {
			window.location.replace("/login");
		}
	</script>
		<MainPage client:load />
</Layout>
